<%
var headData = {
  siteURL:'',
  title:'',
  description:'',
  thumbnail:''
}
-%>
<!DOCTYPE html>
<html lang="ja" prefix="og: http://ogp.me/ns#">
<head>
<title><% if (data.topFlag === true) { %><%= headData.title %><% } else { %><%= data.title %><%= headData.title %><% } %></title>
<meta charset="UTF-8">
<meta http-equiv="X-UA-Compatible" content="IE=Edge">
<meta name="format-detection" content="telephone=no">
<meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
<meta name="description" content="<%= headData.description %>">
<meta property="og:title" content="<% if (data.topFlag === true) { %><%= headData.title %><% } else { %><%= data.title %><%= headData.title %><% } %>" />
<meta property="og:type" content="<% if (data.topFlag === true) { %>website<% } else { %>article<% } %>" />
<meta property="og:url" content="<%= headData.siteURL %><%= data.filePath %>" />
<meta property="og:image" content="<%= headData.siteURL %><%= headData.thumbnail %>" />
<meta property="og:site_name"  content="<% if (data.topFlag === true) { %><%= headData.title %><% } else { %><%= data.title %><%= headData.title %><% } %>" />
<meta property="og:description" content="<%= headData.description %>" />
<meta name="twitter:card" content="summary" />
<meta name="twitter:site" content="@Twitter" />
<meta name="twitter:title" content="<% if (data.topFlag === true) { %><%= headData.title %><% } else { %><%= data.title %><%= headData.title %><% } %>" />
<meta name="twitter:url" content="<%= headData.siteURL %><%= data.filePath %>" />
<meta name="twitter:description" content="<%= headData.description %>" />
<meta name="twitter:image" content="<%= headData.siteURL %><%= headData.thumbnail %>" />
<link rel="stylesheet" href="<%= data.rootPath %>assets/css/style.css">
<script>
  // Picture element HTML5 shiv
  document.createElement( "picture" );
</script>
</head>
<body>
